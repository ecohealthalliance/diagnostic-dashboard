template(name="dash")
  .dashboard-container
    .diagnosis
      +reactiveTable collection=diseases settings=tableSettings
      .symptoms
        each symptoms
          .label(style="background-color:#{color}") #{this}
    .pane-container
      .pane
        +graph
      .pane
        +table
      .pane
        +dendrogram
      .pane
        +testPane
