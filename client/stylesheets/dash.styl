.dashboard-container
  display flex
  flex-direction row
  align-items top
  justify-content flex-end

.diagnosis
  flex 0 0 250px
  background-color orange
  color black

.pane-container
  display flex
  flex-direction row
  align-items left
  flex-wrap wrap

.pane
  flex 0 1 auto
  width 50%
  background-color pink
  color black
  border 3px solid green
  overflow auto

.pane.minimized
  width 33%

.pane.maximized
  width 100%
  order -1
